<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.medappjam">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SettingsActivity"></activity>
        <activity android:name=".ChangePasswordActivity"></activity>
        <activity android:name=".EditProviderInfoActivity"></activity>
        <activity android:name=".ArzangTest"></activity>
        <activity android:name=".RecordsTableActivity"></activity>
        <activity android:name=".NumberInputActivity"></activity>
        <activity android:name=".SignUpActivity"></activity>
        <activity android:name=".LoginActivity"></activity>
        <activity android:name=".HowYouFeelActivity"></activity>

        <meta-data android:name="com.google.android.gms.version" />

        <activity android:name=".MyProvidersActivity"></activity>
        <activity android:name=".AddProviderActivity"></activity>
        <activity android:name=".DeleteRecordsActivity"></activity>
    </application>

</manifest>